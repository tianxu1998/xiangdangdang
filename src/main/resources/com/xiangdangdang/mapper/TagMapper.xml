<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiangdangdang.mapper.TagMapper">
    <select id="findAllTag" resultType="com.xiangdangdang.entity.Tag">
        select * from tag
    </select>

    <select id="findTagById" parameterType="java.lang.Long" resultType="com.xiangdangdang.entity.Tag">
        select * from tag where tag_id=#{tag_id}
    </select>

</mapper>